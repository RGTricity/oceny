<div class='submission-header'>
  <span>Imię</span>
  <span>Wiek</span>
  <span>Średnia ocena</span>
  <span>Ilość ocen</span>
</div>
<ol class='submission-list'>
  {{#each submission in submissions}}
    <li>
      <span> {{submission_link submission}} </span>
      <span> {{submission.age}} </span>
      <span> {{submission.average_rate}} </span>
      <span> {{submission.rates_count}} </span>
    </li>
  {{/each}}
</ol>
